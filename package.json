{
  "name": "TriviaLeaderBoard",
  "version": "1.1.0",
  "description": "Trivia Night Score Entry and Leader Board",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-macOS": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win32": "electron-packager . --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds",
    "package-win64": "electron-packager . --overwrite --platform=win32 --arch=x64 --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shpoellet/TriviaLeaderBoard.git"
  },
  "dependencies": {
    "express": "^4.16.3",
    "npm": "^6.4.1",
    "osc-js": "^1.2.2",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-packager": "^14.2.1"
  }
}
